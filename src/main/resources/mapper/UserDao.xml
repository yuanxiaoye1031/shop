<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.edu.hit.dao.UserDao">

    <insert id="addUser" parameterType="cn.edu.hit.po.User">
        insert into `user` (u_id,username,password,`name`,email,phone,addr,`state`,code) values (default,#{username},#{password},#{name},#{email},#{phone},#{addr},#{state},#{code})
    </insert>

</mapper>